<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="768.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Controllers.MainViews.StaffViews.PreviewTestFormController">
   <children>
      <ComboBox fx:id="SubjectCombo" layoutX="158.0" layoutY="104.0" onAction="#SubjectComboAct" prefWidth="150.0" promptText="Choose Subject" />
      <ComboBox fx:id="CourseCombo" layoutX="463.0" layoutY="104.0" onAction="#CourseComboAct" prefWidth="150.0" promptText="Choose Course" />
      <TableView fx:id="ExamFormTV" layoutX="93.0" layoutY="185.0" onMouseClicked="#ExamFormClick" prefHeight="430.0" prefWidth="611.0">
        <columns>
          <TableColumn fx:id="ExamIDCol" prefWidth="169.0" text="Exam ID" />
          <TableColumn fx:id="CreatorCol" prefWidth="221.0" text="Creator" />
            <TableColumn fx:id="LastUsedCol" prefWidth="210.0" text="Last Used" />
        </columns>
      </TableView>
      <Label layoutX="319.0" layoutY="28.0" prefHeight="34.0" prefWidth="163.0" text="Test Form Preview">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="EditBtn" layoutX="319.0" layoutY="644.0" mnemonicParsing="false" onAction="#EditBtnAct" prefHeight="26.0" prefWidth="52.0" text="Edit" />
      <Button fx:id="DuplicateBtn" layoutX="430.0" layoutY="644.0" mnemonicParsing="false" onAction="#DuplicateAct" text="Duplicate" />
   </children>
</AnchorPane>
